{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mailpoet/html2text"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mailpoet/swiftmailer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "twig/twig": "1.*",
    "cerdic/css-tidy": "^1.6",
    "tburry/pquery": "^1.1.1",
    "j4mie/paris": "1.5.6",
    "swiftmailer/swiftmailer": "dev-tls",
    "mtdowling/cron-expression": "^1.1",
    "nesbot/carbon": "^1.21",
    "soundasleep/html2text": "dev-master",
    "sabberworm/php-css-parser": "^8.1",
    "symfony/polyfill-php72": "^1.8",
    "symfony/polyfill-mbstring": "1.8.0",
    "sensiolabs/security-checker": "^4.1",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "codeception/aspect-mock": "2.2.0",
    "goaop/framework": "2.2.0",
    "codeception/codeception": "2.4.2",
    "codeception/verify": "^0.4.0",
    "consolidation/robo": "^1.3.0",
    "henrikbjorn/lurker": "^1.2",
    "lucatume/wp-browser": "1.21.23",
    "phpunit/phpunit": "5.7.27",
    "vlucas/phpdotenv": "^2.4.0",
    "umpirsky/twig-gettext-extractor": "1.1.*",
    "kint-php/kint": "^2.2",
    "squizlabs/php_codesniffer": "^3.3",
    "phpcompatibility/php-compatibility": "^9.0",
    "coenjacobs/mozart": "^0.2.2"
  },
  "autoload": {
    "psr-4": {
      "MailPoet\\": "lib/",
      "Sudzy\\": "lib/Util/Sudzy"
    }
  },
  "scripts": {
    "post-update-cmd": "\"vendor/bin/mozart\" compose; rm -rf vendor/monolog",
    "post-install-cmd": "\"vendor/bin/mozart\" compose; rm -rf vendor/monolog"
  },
  "extra": {
    "mozart": {
      "dep_namespace": "MailPoet\\Dependencies\\",
      "dep_directory": "/lib/Dependencies/",
      "classmap_directory": "/classes/dependencies/",
      "classmap_prefix": "MP_",
      "packages": [
        "monolog/monolog"
      ]
    }
  },
  "config": {
    "platform": {
      "php": "5.6.30"
    }
  }
}
